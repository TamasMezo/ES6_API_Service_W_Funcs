{
  "name": "express-es6-sample",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "dev": "NODE_ENV=development npm run watch ./src/bin/www",
    "prod": "npm run build && NODE_ENV=production node ./dist/bin/www",
    "build": "npm run clean && babel ./src --config-file ./.babelrc --out-dir dist",
    "clean": "rimraf dist",
    "watch": "nodemon --config ./nodemon.json --"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bigint-hash": "^0.2.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "intl": "^1.2.5",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.4",
    "morgan": "~1.9.0",
    "nodemailer": "^6.4.5",
    "rimraf": "^2.6.3",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.4.4",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "nodemon": "^1.19.0"
  }
}
